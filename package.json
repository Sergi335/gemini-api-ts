{
  "name": "gemini-api-ts",
  "version": "1.0.0",
  "description": "API with Node.js and TypeScript",
  "main": "dist/app.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=test ts-node-dev src/app.ts",
    "production": "cross-env NODE_ENV=production ts-node-dev src/app.ts",
    "tsc": "tsc",
    "start": "node dist/app.js",
    "lint": "ts-standard",
    "lint:fix": "ts-standard --fix",
    "test": "vitest",
    "convert-data": "ts-node src/utils/convertData.ts",
    "import-data": "ts-node src/utils/dataImporter.ts",
    "import-data-overwrite": "ts-node src/utils/dataImporter.ts --overwrite"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.11.0",
    "cheerio": "1.0.0-rc.12",
    "cookie-parser": "^1.4.7",
    "cross-env": "7.0.3",
    "dotenv": "16.0.3",
    "express": "^4.21.2",
    "firebase": "^12.0.0",
    "firebase-admin": "^13.4.0",
    "mongoose": "^6.13.8",
    "multer": "1.4.5-lts.1",
    "nodemon": "^3.1.10",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.9",
    "@types/express": "5.0.3",
    "@types/multer": "^2.0.0",
    "@types/supertest": "6.0.3",
    "supertest": "7.1.1",
    "ts-node-dev": "2.0.0",
    "ts-standard": "12.0.2",
    "typescript": "5.8.3",
    "vitest": "^3.2.4"
  },
  "eslintConfig": {
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "extends": [
      "./node_modules/ts-standard/eslintrc.json"
    ]
  }
}
